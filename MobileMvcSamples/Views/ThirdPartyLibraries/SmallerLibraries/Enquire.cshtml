@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
  <meta name="viewport" content="width=device-width" />
  <title>Enquire</title>
</head>
<body>
  <h1>Enquire</h1>

  <p>There is bacon below this paragraph, but you will only see it if your screen is small enough.</p>

  

  <div id="put-content-here">

  </div>

  <script src="/content/jquery-1-9-0.js"></script>
  <script src="/content/third-party-libraries/enquire/enquire.js"></script>
  <script>

window.addEventListener('load', load);

function load() {

  enquire.register("screen and (max-width:500px)", {

    match: function () {
      $.ajax({
        url: '/thirdpartylibraries/smallerlibraries/enquirecontent',
        success: function (content) {
          console.log('got this', content);
          $('#put-content-here').html(content);
        }
      });
    },

    unmatch: function () {
      $('#put-content-here').html('');
    },

  });
}

  </script>
</body>
</html>
